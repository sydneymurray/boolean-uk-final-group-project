// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW")
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id Int @id @default(autoincrement())
  name String
  username String
  email String @unique
  password String
}